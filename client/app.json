{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "expo-document-picker",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Lemon to use your location."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Lemon to access your camera to scan ID documents."
        }
      ]
    ],
    "name": "Lemon",
    "icon": "./assets/common/logo.png",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c7e609ae-b1e2-4adf-b25e-776ac6dfd8a4"
      }
    },
    "android": {
      "package": "com.oftotech.lemon",
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    }
  }
}
