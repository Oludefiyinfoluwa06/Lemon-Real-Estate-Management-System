{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "expo-document-picker",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Lemon to use your location."
        }
      ]
    ],
    "name": "Lemon",
    "icon": "./assets/common/logo.png",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c7e609ae-b1e2-4adf-b25e-776ac6dfd8a4"
      }
    },
    "android": {
      "package": "com.oftotech.lemon",
      "config": {
        "googleMaps": {
          "apiKey": "@GOOGLE_MAPS_API_KEY"
        }
      }
    }
  }
}
